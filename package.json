{
  "name": "feedbackio-api",
  "version": "0.0.1",
  "description": "A easy way to get and manage your services feedbacks and evaluations.",
  "homepage": "https://github.com/gabriellopes00/feedbackio-api#readme",
  "main": "src/app/server.ts",
  "private": "true",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabriellopes00/feedbackio-api.git"
  },
  "author": {
    "name": "Gabriel Luis Lopes",
    "email": "gabrielluislopes00@gmail.com",
    "url": "https://github.com/gabriellopes00"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/gabriellopes00/feedbackio-api/issues"
  },
  "scripts": {
    "build": "rimraf dist/ && tsc -p tsconfig-build.json --skipLibCheck",
    "start": "node dist/app/server.js",
    "dev": "ts-node-dev  --respawn --transpile-only --ignore-watch node_modules --clear --exit-child --rs --no-notify src/app/server.ts",
    "style": "prettier --check **/*.ts",
    "style:fix": "prettier --write **/*.ts",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "orm": "node ./node_modules/typeorm/cli.js",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand --no-cache",
    "test:cache": "jest --clearCache",
    "test:ci": "yarn test --coverage",
    "test:unit": "yarn test -c jest-unit-config.js",
    "test:e2e": "yarn test -c jest-e2e-config.js",
    "test:coveralls": "yarn test:ci && coveralls < coverage/lcov.info"
  },
  "dependencies": {
    "argon2": "^0.27.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "pg": "^8.5.1",
    "pino": "^6.11.1",
    "pino-pretty": "^4.7.1",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.14.25",
    "@types/pino": "^6.3.6",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "coveralls": "^3.1.0",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sqlite3": "^5.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "_moduleAliases": {
    "@": "dist/"
  }
}