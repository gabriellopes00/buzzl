{
  "name": "feedbackio-api",
  "version": "2.0.0",
  "description": "A easy way to get and manage your services feedbacks and evaluations.",
  "homepage": "https://github.com/gabriellopes00/feedbackio-api#readme",
  "main": "src/app/server.ts",
  "private": "true",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabriellopes00/feedbackio-api.git"
  },
  "author": {
    "name": "Gabriel Luis Lopes",
    "email": "gabrielluislopes00@gmail.com",
    "url": "https://github.com/gabriellopes00"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/gabriellopes00/feedbackio-api/issues"
  },
  "scripts": {
    "build": "rimraf dist/ && tsc -p tsconfig-build.json --skipLibCheck",
    "start": "node ./node_modules/typeorm/cli.js migration:run && node dist/app/server.js",
    "dev": "ts-node-dev  --respawn --transpile-only --ignore-watch node_modules --clear --exit-child --rs --no-notify src/app/server.ts",
    "style": "prettier --check **/*.ts",
    "style:fix": "prettier --write **/*.ts",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "orm": "node ./node_modules/typeorm/cli.js",
    "gen:key:pair": "openssl genrsa -out private_key.pem 1024 && openssl rsa -pubout -in private_key.pem -out public_key.pem",
    "test": "jest --passWithNoTests --noStackTrace --runInBand --no-cache",
    "test:ci": "yarn test --coverage",
    "test:unit": "yarn test -c jest-unit-config.js",
    "test:e2e": "yarn test -c jest-e2e-config.js"
  },
  "dependencies": {
    "@sentry/node": "^6.9.0",
    "argon2": "^0.28.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.3.0",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "pg": "^8.5.1",
    "pino": "^6.13.3",
    "pino-pretty": "^4.7.1",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.37",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bull": "^3.15.1",
    "@types/express": "^4.17.13",
    "@types/express-pino-logger": "^4.0.2",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^14.14.25",
    "@types/nodemailer": "^6.4.2",
    "@types/pino": "^6.3.11",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.32.0",
    "coveralls": "^3.1.1",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^7.0.2",
    "jest": "^26.6.3",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "sqlite3": "^5.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^26.5.0",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.4.2"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "_moduleAliases": {
    "@": "dist/"
  }
}
